    "pinia": "^3.0.2",
    "postcss": "^8.5.3",
    "sass": "^1.87.0",
    "uuid": "^11.1.0",
    "vuefire": "^3.2.2"
  },
  "resolutions": {
    "undici": "^6.6.2",
    "@grpc/grpc-js": "^1.10.9",
